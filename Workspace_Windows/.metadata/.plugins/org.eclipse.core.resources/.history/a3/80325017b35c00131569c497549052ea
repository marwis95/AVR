#include<io/avr.h>

int main(){

	 DDRA = 0xFF;
	   DDRB = 0xFF;
	   DDRC = 0xFF;
	   DDRD = 0xFF;

	   /* Pocz¹tek nieskoñczonej pêtli */
	   while(1)
	   {
		PORTA=0xFF;
		PORTB=0xFF;
		PORTC=0xFF;
		PORTD=0xFF;
	   }

	return 0;
}
